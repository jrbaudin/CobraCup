                                    <img src="../img/hockeyplayer.png" class="img-responsive" alt="Responsive image" style="min-height:75px;height:75px; margin: 0 auto;">
                                <% } %>
                                <h5 class="text-center" title="<%= teamObj.get("lieutenant").get("name") %>"><%= getFirstname(teamObj.get("lieutenant").get("name")) %> <small style="color: rgb(0,0,0);">Assisterande</small></h5>
                            </a>
                            <p class="text-center" style="line-height: 1; margin: 0;"><%= teamObj.get("lieutenant").get("telephone") %></p>
                            
                            <% if ((typeof(teamObj.get("lieutenant").get("gamertag")) !== 'undefined') && (!_.isEmpty(teamObj.get("lieutenant").get("gamertag")))) { %>
                                <p class="text-center" style="line-height: 1; margin: 0;" title="Xbox Live Gamertag"><small><i class="fa fa-gamepad" style="color: rgb(0,0,0);"></i> <span style="color: rgb(16,124,16);"><strong><%= teamObj.get("lieutenant").get("gamertag") %></strong></span></small></p>
                            <% } %>

                            <% if ((typeof(teamObj.get("lieutenant").get("psn_id")) !== 'undefined') && (!_.isEmpty(teamObj.get("lieutenant").get("psn_id")))) { %>
                                <p class="text-center" style="line-height: 1; margin: 0;" title="PlayStation Network ID"><small><i class="fa fa-gamepad" style="color: rgb(0,0,0);"></i> <span style="color: #003791;"><strong><%= teamObj.get("lieutenant").get("psn_id") %></strong></span></small></p>
                            <% } %>
                        </div>
                        <hr>
                        <!--
                        <div class="col-md-6 col-md-offset-3">
                            <p class="text-center lead"><i>Lagmotto</i></p>
                            <blockquote>
                                <p><%= teamObj.get("team_motto") %></p>
                                <small><%= teamObj.get("team_name") %></small>
                            </blockquote>
                        </div>
                        -->
                        <div class="col-md-8 col-md-offset-2">
                            <p class="text-center lead" style="margin-bottom: 5px;"><span class="fui-list"></span> Nivå <span class="fui-list"></span></p>
                            <div class="progress" style="height: 20px;">
                            <% if(_.isEqual(teamObj.get("level"), "1")) { %>
                                <div class="progress-bar" style="width: 35%; line-height: 20px;"><i class="fa fa-child"></i> Rookie</div>
                            <% } else if(_.isEqual(teamObj.get("level"), "2")) { %>
                                <div class="progress-bar progress-bar-success" style="width: 65%; line-height: 20px;"><i class="fa fa-gamepad"></i> Pro</div>
                            <% } else { %>
                                <div class="progress-bar progress-bar-warning" style="width: 100%; line-height: 20px;"><i class="fa fa-trophy"></i> All Star</div>
                            <% } %>
                            </div>
                        </div>
                        <% if (typeof teamObj.get("legend") !== 'undefined') { %>
                          <div class="col-md-8 col-md-offset-2">
                            <p class="text-center lead" style="margin-bottom: 5px;"><i class="fa fa-trophy"></i> <i>Placeringar i Cobra Cup</i></p>

                            <% if ( (!_.isEmpty(teamObj.get("legend")[2])) && (!_.isEqual(teamObj.get("legend")[2], "-")) ) { %>
                                <p class="text-center" style="line-height: 2; margin: 0;"><strong>2015</strong> - <%= teamObj.get("legend")[2] %></p>
                            <% } %>

                            <% if ( (!_.isEmpty(teamObj.get("legend")[1])) && (!_.isEqual(teamObj.get("legend")[1], "-")) ) { %>
                                <p class="text-center" style="line-height: 2; margin: 0;"><strong>2014</strong> - <%= teamObj.get("legend")[1] %></p>
                            <% } %>

                            <% if ( (!_.isEmpty(teamObj.get("legend")[0])) && (!_.isEqual(teamObj.get("legend")[0], "-")) ) { %>
                                <p class="text-center" style="line-height: 2; margin: 0;"><strong>2010</strong> - <%= teamObj.get("legend")[0] %></p>
                            <% } %>
                            
                            <!--
                            <% if (typeof teamObj.get("marathon") !== 'undefined') { %>
                              <p class="text-center" title="Legend för lagen. Baserat på matcher spelade 2014 & 2015" style="line-height: 2; margin: 0;">
                                <small>W:<strong><%= teamObj.get("marathon")[0] %></strong> - T:<strong><%= teamObj.get("marathon")[1] %></strong> - L:<strong><%= teamObj.get("marathon")[2] %></strong></small>
                                <a href="#" style="margin-left: 15px;"data-toggle="tooltip" title="" data-original-title="Baserat på matcher spelade i gruppspel 2014 & 2015." data-placement="right"><i class="fa fa-info-circle"></i></a>
                              </p>
                            <% } %>
                            -->
                          </div>
                        <% } %>
                        <!-- <div class="clearfix"></div>
                        <div class="col-md-12">
                            <p class="text-center" style="font-size: 12px; opacity: 0.7;"><small><span class="fui-new"></span> Redigera</small></p>
                        </div> -->

                        <div class="col-md-12">
                            <% var stats = teamObj.get("stats"); %>

                            <p class="text-uppercase" style="margin-bottom:2px;"><strong>Säsonger</strong></p>
                            <div class="table-responsive" style="margin-bottom:30px;">
                                <table class="table table-bordered table-condensed table-striped" style="margin-bottom: 0;">
                                    <thead class="small-table-header">
                                        <tr class="statstable">
                                            <th class="text-uppercase">Säsong</th>
                                            <th class="text-center text-uppercase" title="Games Played">GP</th>
                                            <th class="text-center text-uppercase" title="Wins">W</th>
                                            <th class="text-center text-uppercase" title="Ties">T</th>
                                            <th class="text-center text-uppercase" title="Losses">L</th>
                                            <th class="text-center text-uppercase" title="Points">P</th>
                                            <th class="text-center text-uppercase" title="Goals For">GF</th>
                                            <th class="text-center text-uppercase" title="Goals Against">GA</th>
                                            <th class="text-center text-uppercase" title="Difference">+/-</th>
                                            <th></th>
                                            <th class="text-uppercase" title="Slutspel">POST</th>
                                            <th class="text-center text-uppercase" title="Games Played">GP</th>
                                            <th class="text-center text-uppercase" title="Wins">W</th>
                                            <th class="text-center text-uppercase" title="Losses">L</th>
                                            <th class="text-center text-uppercase" title="Goals For">GF</th>
                                            <th class="text-center text-uppercase" title="Goals Against">GA</th>
                                            <th class="text-center text-uppercase" title="Difference">+/-</th>
                                        </tr>
                                    </thead>
                                    <tbody class="small-table-rows">
                                        <% if( typeof(stats) !== 'undefined' ){ %>
                                        
                                            <% _.each(stats.seasons, function(season) { %>
                                                <% var difference = season.gf-season.ga; %>
                                                <tr>
                                                    <td><%= season.season %></td>
                                                    <td class="text-center"><%= season.gp %></td>
                                                    <td class="text-center"><%= season.wins %></td>
                                                    <td class="text-center"><%= season.ties %></td>
                                                    <td class="text-center"><%= season.losses %></td>
                                                    <td class="text-center"><%= season.points %></td>
                                                    <td class="text-center"><%= season.gf %></td>
                                                    <td class="text-center"><%= season.ga %></td>
                                                    <td class="text-center"><%= difference %></td>
                                                    <td class="text-center">|</td>
                                                    <% if ((typeof(season.playoffs) !== 'undefined') && (season.playoffs.gp > 0)) { %>
                                                        <% var po_difference = season.playoffs.gf-season.playoffs.ga; %>
                                                        <td><i class="fa fa-arrow-right"></i> Slutspel</td>
                                                        <td class="text-center"><%= season.playoffs.gp %></td>
                                                        <td class="text-center"><%= season.playoffs.wins %></td>
                                                        <td class="text-center"><%= season.playoffs.losses %></td>
                                                        <td class="text-center"><%= season.playoffs.gf %></td>
                                                        <td class="text-center"><%= season.playoffs.ga %></td>
                                                        <td class="text-center"><%= po_difference %></td>
                                                    <% } else { %>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                    <% } %>
                                                </tr>
                                            <% })%> 

                                        <% } %>
                                    </tbody>
                                </table>
                            </div>
                            <small style="font-size: 10px;"><strong>GP</strong>-Games Played, <strong>W</strong>-Wins, <strong>T</strong>-Ties, <strong>L</strong>-Losses, <strong>P</strong>-Points, <strong>GF</strong>-Goals For, <strong>GA</strong>-Goals Against, <strong>+/-</strong> -Goal Difference</small>
                        </div>

                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>

            <div class="col-md-4 col-md-pull-8">
                <div class="row">
                    <div class="panel">
                        <div class="panel-body">
                            <img src="<%= teamObj.get("nhlTeam").get("logo").url() %>" class="img-responsive img-circle" alt="Responsive image" style="min-height:150px;height:150px; margin: 0 auto;">
                            <h4 class="text-center"><%= teamObj.get("nhlTeam").get("name") %></h4>
                            <h6 class="text-center"><%= teamObj.get("nhlTeam").get("city") %></h4>
                            <a class="btn btn-primary btn-md center-block" href="<%= teamObj.get("nhlTeam").get("homepage") %>" target="_blank" onclick="_gs('event', 'Clicked to see NHL team homepage')">Hemsida</a>
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <div class="row">
                    <div class="panel panel-default" style="border: 0; background-color: transparent; margin-bottom: 0;">
                        <div class="panel-body" style="padding: 0;">
                            <div class="panel-group" id="accordion">

                                <% if((typeof(lastPlayedGame) !== 'undefined') && (typeof(teams) !== 'undefined')){
                                    var hometeam = null;
                                    var awayteam = null;
                                    var versus = "";
                                    teams.forEach(function(team) {
                                        if(_.isEqual(team.id, lastPlayedGame.get("home").id)){
                                            hometeam = team;
                                            if(!_.isEqual(team.id, teamObj.id)){
                                                versus = team.get("nhlTeam").get("nickname");
                                            }
                                        } else if(_.isEqual(team.id, lastPlayedGame.get("away").id)){
                                            awayteam = team;
                                            if(!_.isEqual(team.id, teamObj.id)){
                                                versus = team.get("nhlTeam").get("nickname");
                                            }
                                        }
                                    })
                                %>
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        <h6 class="panel-title text-uppercase">
                                            <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne"><span class="fui-triangle-down"></span> Senaste match VS. <%= versus %></a>
                                        </h6>
                                    </div>
                                    <div id="collapseOne" class="panel-collapse collapse in">
                                        <div class="panel-body">
                                            <div class="col-md-12">
                                                <a href="/stat/game/<%= lastPlayedGame.get("game_id") %>" style="color: rgb(241,102,57);"><p class="text-center text-uppercase" style="margin-bottom: 0; font-weight: bold;">Grupp <%= lastPlayedGame.get("group") %>, Runda <%= lastPlayedGame.get("round") %></p></a>
                                            </div>
                                            <div class="row">
                                                <div class="col-xs-4 col-md-4">
                                                    <img src="<%= hometeam.get("nhlTeam").get("logo").url() %>" class="img-responsive img-circle" alt="Responsive image" style="min-height:50px;height:50px; margin: 0 auto;">
                                                </div>
                                                <div class="col-xs-4 col-md-4">
                                                    <p class="text-center" style="margin-bottom: 0; line-height: 1;"><small>SLUT</small></p>
                                                    <p class="text-center" style="line-height: 1;"><b><%= lastPlayedGame.get("home_goals_total") %> - <%= lastPlayedGame.get("away_goals_total") %></b></p>
                                                </div>
                                                <div class="col-xs-4 col-md-4">
                                                    <img src="<%= awayteam.get("nhlTeam").get("logo").url() %>" class="img-responsive img-circle" alt="Responsive image" style="min-height:50px;height:50px; margin: 0 auto;">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <% } else { %>
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h6 class="panel-title text-uppercase">
                                                <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne"><span class="fui-alert-circle"></span> Senaste match VS.</a>
                                            </h6>
                                        </div>
                                        <div id="collapseOne" class="panel-collapse collapse in">
                                            <div class="panel-body">
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <p class="text-center" style="margin-bottom: 0; line-height: 1;">Problem att ladda senaste match eller så har detta lag inte spelat några matcher.</p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                <% } %>

                                <% if((typeof(games) !== 'undefined') && (typeof(teams) !== 'undefined')){
                                    var nextGame = _.first(games);
                                }
                                var hometeam = null;
                                var awayteam = null;
                                var versus = "";
                                var irl_nickname = "";
                                if(typeof(nextGame) !== 'undefined'){
                                    teams.forEach(function(team) {
                                        if(_.isEqual(team.id, nextGame.get("home").id)){
                                            hometeam = team;
                                            if(!_.isEqual(team.id, teamObj.id)){
                                                versus = team.get("nhlTeam").get("nickname");
                                                irl_nickname = team.get("nickname");
                                            }
                                        } else if(_.isEqual(team.id, nextGame.get("away").id)){
                                            awayteam = team;
                                            if(!_.isEqual(team.id, teamObj.id)){
                                                versus = team.get("nhlTeam").get("nickname");
                                                irl_nickname = team.get("nickname");
                                            }
                                        }
                                    }) %>
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <h6 class="panel-title text-uppercase">
                                                <a data-toggle="collapse" data-parent="#accordion" href="#collapseTwo"><span class="fui-triangle-down"></span> Nästa match VS. <%= versus %></a>
                                            </h6>
                                        </div>
                                        <div id="collapseTwo" class="panel-collapse collapse">
                                            <div class="panel-body">
                                                <div class="row">
