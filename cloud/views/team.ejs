                    <span aria-hidden="true">&times;</span>
                    <span class="sr-only">Close</span>
                </button>
                <strong>Problem!</strong> <%= flash %>.
            </div>
        <% } %>
        <% if(typeof(teamObj) !== 'undefined'){ %>
                
