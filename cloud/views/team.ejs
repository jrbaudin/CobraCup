                                <% } else { %>
                                    <img src="../img/hockeyplayer.png" class="img-responsive" alt="Responsive image" style="min-height:75px;height:75px; margin: 0 auto;">
                                <% } %>
                                <h5 class="text-center" title="<%= teamObj.get("captain").get("name") %>"><%= getFirstname(teamObj.get("captain").get("name")) %> <small style="color: rgb(0,0,0);">Kapten</small></h5>
                            </a>
                            <p class="text-center" style="line-height: 1; margin: 0;"><%= teamObj.get("captain").get("telephone") %></p>

                            <% if ((typeof(teamObj.get("captain").get("gamertag")) !== 'undefined') && (!_.isEmpty(teamObj.get("captain").get("gamertag")))) { %>
