    <div class="container-fluid">
        <% if(typeof(flash) !== 'undefined'){ %>
            <div class="alert alert-danger alert-dismissible" role="alert">
                <button type="button" class="close" data-dismiss="alert">
                    <span aria-hidden="true">&times;</span>
                    <span class="sr-only">Close</span>
                </button>
                <strong>Fel!</strong> <%= flash %>.
            </div>
        <% } %>
        <div class="col-md-3">
            <div class="panel panel-info">
                <div class="panel-heading">
                    <h6 class="panel-title text-uppercase">
                        <span class="fa-stack fa-lg">
                          <i class="fa fa-circle fa-stack-2x"></i>
                          <i class="fa fa-tint fa-stack-1x fa-inverse"></i>
                        </span> <strong>Atlantic Division</strong>
                    </h6>
                </div>
                <div class="panel-body">
                    <button  onclick="window.location.href='/games/group/1'" class="btn btn-hg btn-inverse btn-wide btn-block text-uppercase">Se matcher</button>
                    <% if(typeof(games) !== 'undefined'){
                        var size = _.size(games);
                        if (size > 0) {
                            games.forEach(function loop(game) { 
                                if(loop.stop){ return; } %>
                                <% if(_.isEqual(game.get("group"), 1)){ %>
                                    <a href="/stat/game/<%= game.get("game_id") %>" class="colormedark">
                                        <div class="row colormybackground givesomespace-top" style="margin-bottom: 0;">
                                            <p class="text-center text-uppercase" style="margin-bottom: 5px;"><strong>Nästa match</strong></p>
                                        </div>
                                        <div class="row colormybackground givesomespace-bottom">
                                            <div class="col-xs-4 col-md-2" style="padding-right: 5px;">
                                                <img src="<%= game.get("home").get("nhlTeam").get("logo").url() %>" class="img-responsive img-thumbnail" alt="Home Team Logo" style="min-height:40px;height:40px; margin: 0 auto;">
                                            </div>
                                            <div class="col-xs-4 col-md-8">
                                                <p class="text-center text-uppercase" style="margin-bottom: 0; line-height: 1;">Runda <strong><%= game.get("round") %></strong></p>
                                                <p class="text-center text-uppercase" style="margin-bottom: 0; line-height: 1;"><small><%= game.get("home").get("nickname") %> vs. <%= game.get("away").get("nickname") %></small></p>
                                            </div>
                                            <div class="col-xs-4 col-md-2" style="padding-left: 5px;">
                                                <img src="<%= game.get("away").get("nhlTeam").get("logo").url() %>" class="img-responsive img-thumbnail" alt="Away Team Logo" style="min-height:40px;height:40px; margin: 0 auto;">
                                            </div>
                                        </div>
                                    </a>
                                    <% loop.stop = true;
                                }
                            }) %>
                        <% } %>
                    <% } %>
                    <% if(typeof(teams) !== 'undefined'){ %>
                        <p class="text-center text-uppercase" style="margin-bottom: 10px;"><strong>Lag i gruppen</strong></p>
                        <% teams.forEach(function(team) {
                            if(_.isEqual(team.get("group"), "1")){ %>
                                <p class="text-uppercase text-center"><a href="/team/<%= team.get("team_id") %>"><%= team.get("team_name") %></a></p>
                            <% }
                        }) %>
                    <% } %>
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-md-3 -->
