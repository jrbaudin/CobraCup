<!DOCTYPE html>
<html lang="en">
 
<head>
    <!-- Include header section which holds imports for shared elements -->  
    <% include _header %>
 
    <title>Grupper - Cobra Cup 2015</title>
 
    <!-- Latest compiled and minified CSS -->
    <link href="../dist/css/vendor/bootstrap.min.css" rel="stylesheet">

    <!-- Optional theme -->
    <link rel="stylesheet" href="../dist/css/flat-ui.css">

    <link rel="stylesheet" href="../stylesheets/style.css">
</head>
 
<body>
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
              <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
              <a class="navbar-brand" href="/"><i class="fa fa-cube"></i> Hub <span style="font-family: 'Kaushan Script', cursive;">2016</span></a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
              <ul class="nav navbar-nav">
                <li><a href="/">Lag</a></li>
                <li><a href="/signup">Anm√§l lag</a></li>
                <li class="dropdown">
                  <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Tabeller <span class="caret"></span></a>
                  <ul class="dropdown-menu" role="menu">
                    <li><a href="/stat/league">Ligan</a></li>
                    <li class="active"><a href="/stat/divisions">Divisioner</a></li>                  
                    <li><a href="/stat/playerstats">Spelarstatistik</a></li>
                  </ul>
                </li>
                <li class="active"><a href="/games">Matcher</a></li>
                <li><a href="/feed"><mark>Live</mark></a></li>
                <li><a href="http://cobracup.se" target="_blank"><i class="fa fa-home"></i> cobracup.se</a></li>
              </ul>
              <% if(typeof(count) !== 'undefined'){ %>
                <p class="navbar-text navbar-right"><strong><%= count %>/15 lag</strong></p>
              <% } %>
            </div><!--/.nav-collapse -->
        </div>
    </nav>
    <div class="container-fluid">
        <% if(typeof(flash) !== 'undefined'){ %>
            <div class="alert alert-danger alert-dismissible" role="alert">
                <button type="button" class="close" data-dismiss="alert">
                    <span aria-hidden="true">&times;</span>
                    <span class="sr-only">Close</span>
                </button>
                <strong>Fel!</strong> <%= flash %>.
            </div>  
        <% } %>
        <div class="col-md-3">
            <div class="panel panel-info">
                <div class="panel-heading">
                    <h6 class="panel-title text-uppercase">
                        <span class="fa-stack fa-lg">
                          <i class="fa fa-circle fa-stack-2x"></i>
                          <i class="fa fa-tint fa-stack-1x fa-inverse"></i>
                        </span> <strong>Atlantic Division</strong>
                    </h6>
                </div>
                <div class="panel-body">
                    <button  onclick="window.location.href='/games/group/1'" class="btn btn-hg btn-inverse btn-wide btn-block text-uppercase">Se matcher</button>
                    <hr>
                    <% if(typeof(teams) !== 'undefined'){ 
                        teams.forEach(function(team) {
                            if(_.isEqual(team.get("group"), "1")){ %>
                                <p class="text-uppercase text-center"><a href="/team/<%= team.get("team_id") %>"><%= team.get("team_name") %></a></p>
                            <% }
                        })
                    %>

                    <% } %>
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-md-3 -->
        <div class="col-md-3">
            <div class="panel panel-danger">
                <div class="panel-heading">
                    <h6 class="panel-title text-uppercase">
                        <span class="fa-stack fa-lg">
                          <i class="fa fa-circle fa-stack-2x"></i>
                          <i class="fa fa-building fa-stack-1x fa-inverse"></i>
                        </span> <strong>Metropolitan Division</strong>
                    </h6>
                </div>
                <div class="panel-body">
                    <button  onclick="window.location.href='/games/group/2'" class="btn btn-hg btn-inverse btn-wide btn-block text-uppercase">Se matcher</button>
                    <hr>
                    <% if(typeof(teams) !== 'undefined'){ 
                        teams.forEach(function(team) {
                            if(_.isEqual(team.get("group"), "2")){ %>
                                <p class="text-uppercase text-center"><a href="/team/<%= team.get("team_id") %>"><%= team.get("team_name") %></a></p>
                            <% }
                        })
                    %>

                    <% } %>
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-md-3 -->
        <div class="col-md-3">
            <div class="panel panel-success">
                <div class="panel-heading">
                    <h6 class="panel-title text-uppercase">
                        <span class="fa-stack fa-lg">
                          <i class="fa fa-circle fa-stack-2x"></i>
                          <i class="fa fa-bolt fa-stack-1x fa-inverse"></i>
                        </span> <strong>Central Division</strong>
                    </h6>
                </div>
                <div class="panel-body">
                    <button  onclick="window.location.href='/games/group/3'" class="btn btn-hg btn-inverse btn-wide btn-block text-uppercase">Se matcher</button>
                    <hr>
                    <% if(typeof(teams) !== 'undefined'){ 
                        teams.forEach(function(team) {
                            if(_.isEqual(team.get("group"), "3")){ %>
                                <p class="text-uppercase text-center"><a href="/team/<%= team.get("team_id") %>"><%= team.get("team_name") %></a></p>
                            <% }
                        })
                    %>

                    <% } %>
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-md-3 -->
        <div class="col-md-3">
            <div class="panel panel-warning">
                <div class="panel-heading">
                    <h6 class="panel-title text-uppercase">
                        <span class="fa-stack fa-lg">
                          <i class="fa fa-circle fa-stack-2x"></i>
                          <i class="fa fa-sun-o fa-stack-1x fa-inverse"></i>
                        </span> <strong>Pacific Division</strong>
                    </h6>
                </div>
                <div class="panel-body">
                    <button  onclick="window.location.href='/games/group/4'" class="btn btn-hg btn-inverse btn-wide btn-block text-uppercase">Se matcher</button>
                    <hr>
                    <% if(typeof(teams) !== 'undefined'){ 
                        teams.forEach(function(team) {
                            if(_.isEqual(team.get("group"), "4")){ %>
                                <p class="text-uppercase text-center"><a href="/team/<%= team.get("team_id") %>"><%= team.get("team_name") %></a></p>
                            <% }
                        })
                    %>

                    <% } %>
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-md-3 -->
    </div>
    <!-- /.container -->

    <p class="text-center" style="margin-top: 2.5em;"><small>&copy; 2014 Copyright The Cobra Cup</small></p>

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="..//dist/js/vendor/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="..//dist/js/flat-ui.min.js"></script>

    <!-- jQuery Version 1.11.0 -->
    <!-- <script src="//code.jquery.com/jquery-1.11.0.min.js"></script> -->

    <!-- Latest compiled and minified JavaScript -->
    <!-- <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script> -->
</body>
 
</html>