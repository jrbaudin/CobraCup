            <div class="panel panel-success">
                <div class="panel-heading">
                    <h6 class="panel-title text-uppercase">
                        <span class="fa-stack fa-lg">
                          <i class="fa fa-circle fa-stack-2x"></i>
                          <i class="fa fa-bolt fa-stack-1x fa-inverse"></i>
                        </span> <strong>Central Division</strong>
                    </h6>
                </div>
                <div class="panel-body">
                    <button  onclick="window.location.href='/games/group/3'" class="btn btn-hg btn-inverse btn-wide btn-block text-uppercase">Gå till grupp</button>
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-md-3 -->
        <div class="col-md-3">
            <div class="panel panel-warning">
                <div class="panel-heading">
                    <h6 class="panel-title text-uppercase">
                        <span class="fa-stack fa-lg">
                          <i class="fa fa-circle fa-stack-2x"></i>
                          <i class="fa fa-sun-o fa-stack-1x fa-inverse"></i>
                        </span> <strong>Pacific Division</strong>
                    </h6>
                </div>
                <div class="panel-body">
